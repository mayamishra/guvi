#include<stdio.h>
#include<ctype.h>
#include<string.h>
int main()
{
    char ch;
    scanf("%c",&ch);
    if(!isalpha(ch))
    printf("Invalid Input");
    else
    {
        if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
        printf("Vowel");
        else
        printf("Consonant");
    }
return 0;
}
